t window W_MAIN 
  str 'Example window'
  x 10
  y 1
  w 300
  h 200
  min_w 300
  min_h 200
  child M_MAIN

t map M_MAIN
  w auto
  h auto
  row O_Q
  row I_A
  row B_CONFIRM B_NOPE

t otext O_Q
  str 'How old are you?'

t itext I_A
  type '*'
  str '18'

t button B_CONFIRM
  max_w 300
  str 'Confirm'

t button B_NOPE
  inherit B_CONFIRM
  str 'Nope'
